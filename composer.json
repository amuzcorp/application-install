{
    "name": "amuz/application-install",
    "description": "for application download (ios/android) with Release Model",
    "type": "amuz-theme",
    "authors": [
        {
            "name": "Xiso",
            "email": "ceo@amuz.co.kr"
        }
    ],
    "require": {
        "stepanenko3/nova-markdown": "*"
    },
    "autoload": {
        "psr-4": {
            "AmuzThemes\\ApplicationInstall\\": "src/",
            "AmuzThemes\\ApplicationInstall\\Database\\Factories\\": "src/database/factories/",
            "AmuzThemes\\ApplicationInstall\\Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmuzThemes\\ApplicationInstall\\ApplicationInstallThemeProvider"
            ]
        },
        "theme": {
            "parent": "",
            "active": true,
            "screenshot": "screenshot.png"
        }
    }
}
